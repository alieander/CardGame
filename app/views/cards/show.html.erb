<div id="container">
  <div id="sidebar">
    <div><%=request.path_parameters[:action].capitalize%></div>
    <h1 id="num">
      <%=@card.first.capitalize%>
    </h1>
  </div>
  <div id="image">
    <a href="<%=@card.last%>">
    <%= image_tag "magic_img/%s.jpg" % @card.id.to_s %>
    </a>
  </div>
</div>
